# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# GitHub Sync Configuration (production only)
# Required for automatic frontend data updates
# Create a fine-grained PAT with contents:write permission
GITHUB_TOKEN=your_github_pat_here
GITHUB_REPO=owner/repo

# Timezone for correct week number and log timestamps
# Important: Coolify cron runs in UTC by default
TZ=Europe/Berlin

# Optional: Logging Level (default: INFO)
LOG_LEVEL=INFO
